{
  "name": "nestjs-request-deduplication",
  "version": "0.0.0",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/types/index.d.ts"
    }
  },
  "workspaces": [
    "examples/*"
  ],
  "scripts": {
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prepare": "husky install",
    "build": "npm run build:cjs && npm run build:esm",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:esm": "tsc -p tsconfig.esm.json",
    "clean": "rimraf dist",
    "start:memory": "npm run clean && npm run build && npm run dev --workspace=examples/memory",
    "lint": "eslint \"src/**/*.ts\"",
    "lint:fix": "eslint \"src/**/*.ts\" --fix",
    "format": "prettier --write \"src/**/*.ts\"",
    "format:check": "prettier --check \"src/**/*.ts\"",
    "format:fix": "npm run format && npm run lint:fix",
    "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
    "lint-staged": "lint-staged"
  },
  "author": "",
  "license": "ISC",
  "description": "",
  "peerDependencies": {
    "@nestjs/common": "^11.0.0",
    "@nestjs/core": "^11.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.1",
    "@commitlint/config-conventional": "^18.6.1",
    "@nestjs/cli": "^11.0.2",
    "@nestjs/platform-express": "^11.0.8",
    "@nestjs/testing": "^11.0.8",
    "@types/express": "^5.0.0",
    "@types/jest": "^29.5.14",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "^8.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.3",
    "fastify": "^5.2.1",
    "husky": "^8.0.0",
    "jest": "^29.7.0",
    "lint-staged": "^14.0.1",
    "prettier": "^3.5.0",
    "reflect-metadata": "^0.2.2",
    "rimraf": "^6.0.1",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.0",
    "zod": "^3.24.1"
  },
  "lint-staged": {
    "src/**/*.ts": [
      "npm run lint",
      "npm run format:check"
    ]
  },
  "dependencies": {
    "@keyv/memcache": "^2.0.1",
    "@keyv/redis": "^4.2.0",
    "rxjs": "^7.8.0"
  },
  "publishConfig": {
    "access": "public",
    "registry": "https://registry.npmjs.org/",
    "tag": "latest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/daniyel/nestjs-request-deduplication.git"
  },
  "keywords": [
    "nestjs",
    "request",
    "deduplication",
    "interceptor",
    "redis",
    "memcached"
  ],
  "bugs": {
    "url": "https://github.com/daniyel/nestjs-request-deduplication/issues"
  },
  "homepage": "https://github.com/daniyel/nestjs-request-deduplication#readme",
  "engines": {
    "node": ">=23.0.0",
    "npm": ">=10.0.0"
  }
}
